  "services": {
    "call_update": {
      "name": "Run DynDNS update",
      "description": "Calls the DynDNS Manager update URL. IPv4/IPv6 are only sent if the corresponding toggle is enabled. If enabled and the field is empty, the external address will be automatically detected.",
      "fields": {
        "ha_host": {
          "name": "Home Assistant host",
          "description": "Optional. Defaults to the current Home Assistant hostname."
        },
        "ha_port": {
          "name": "Home Assistant port",
          "description": "Optional. Defaults to the current port (usually 8123)."
        },
        "web_username": {
          "name": "Web username",
          "description": "Credentials for web access. If exactly one entry exists, its credentials are used automatically."
        },
        "web_password": {
          "name": "Web password",
          "description": "Credentials for web access. If exactly one entry exists, its credentials are used automatically."
        },
        "use_ipv4": {
          "name": "Use IPv4",
          "description": "Enable to include IPv4. If enabled and the field below is empty, the external IPv4 will be auto-detected."
        },
        "ipv4": {
          "name": "IPv4 address",
          "description": "Optional. Only used if 'Use IPv4' is enabled. Leave empty to auto-detect the external IPv4."
        },
        "use_ipv6": {
          "name": "Use IPv6",
          "description": "Enable to include IPv6. If enabled and the field below is empty, the external IPv6 will be auto-detected."
        },
        "ipv6": {
          "name": "IPv6 address",
          "description": "Optional. Only used if 'Use IPv6' is enabled. Leave empty to auto-detect the external IPv6."
        },
        "timeout": {
          "name": "Timeout",
          "description": "Timeout for the HTTP request in seconds."
        }
      }
    }
  }
